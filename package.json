{
  "name": "gatsby-source-wordpress-graphql",
  "description": "Source data from Wordpress in an efficient and scalable way.",
  "author": "Tobias Strebitzer <tobias.strebitzer@magloft.com>",
  "main": "index.js",
  "version": "3.0.0-next.4",
  "bugs": {
    "url": "https://github.com/magloft/gatsby-source-wordpress-graphql/issues"
  },
  "homepage": "https://github.com/magloft/gatsby-source-wordpress-graphql/tree/master#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-wordpress-graphql",
    "wordpress",
    "graphql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magloft/gatsby-source-wordpress-graphql.git"
  },
  "scripts": {
    "prepack": "yarn build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "build": "rimraf dist; babel src --out-dir dist --extensions '.ts,.js,.tsx' --source-maps",
    "watch": "yarn build; babel -w src --out-dir dist --verbose --extensions '.ts,.js,.tsx' --source-maps",
    "example:clean": "yarn example:gatsby:clean; yarn example:wordpress:clean",
    "example:gatsby:clean": "cd example; yarn gatsby clean",
    "example:wordpress:clean": "rm -rf example/.wordpress-graphql-cache",
    "example:build": "cd example/; yarn; gatsby build --prefix-paths",
    "example:deploy": "yarn example:build; gh-pages -d example/public",
    "generate:graphql": "graphql-codegen --config src/graphql/codegen.js"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0",
    "gatsby-plugin-sharp": "^3.0.0-next.0",
    "gatsby-source-filesystem": "^3.0.0-next.0",
    "gatsby-transformer-sharp": "^3.0.0-next.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@babel/runtime": "^7.14.6",
    "@graphql-codegen/typed-document-node": "^1.18.10",
    "@graphql-tools/links": "7.1.0",
    "@graphql-tools/utils": "7.7.3",
    "@graphql-tools/wrap": "7.0.5",
    "apollo-link": "1.2.14",
    "apollo-link-http": "^1.5.17",
    "async-sema": "^3.1.0",
    "cross-fetch": "^3.1.4",
    "file-type": "^16.5.1",
    "fs-extra": "^9.0.1",
    "gatsby-plugin-image": "^1.9.0",
    "graphql": "^15.5.0",
    "html-react-parser": "^1.2.7",
    "html-to-react": "^1.4.5",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.2.0",
    "unihtml": "~1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.0",
    "@babel/eslint-plugin": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.14.0",
    "@babel/plugin-proposal-private-methods": "^7.14.0",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-type-graphql": "^1.18.9",
    "@types/file-type": "^10.9.1",
    "@types/fs-extra": "^9.0.12",
    "@types/md5": "^2.3.1",
    "@types/node-fetch": "^2.5.11",
    "@types/object-hash": "^2.1.1",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-plugin-import-globals": "^2.0.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-preset-gatsby": "^1.10.0-next.1",
    "babel-preset-gatsby-package": "^1.10.0-next.1",
    "eslint": "^7.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "gatsby": "next",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-sharp": "next",
    "gh-pages": "^3.2.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
